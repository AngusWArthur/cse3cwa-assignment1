generator client {
  provider = "prisma-client-js"

  binaryTargets = ["native", "rhel-openssl-3.0.x", "linux-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model TabSet {
  id        String   @id @default(uuid())
  title     String
  tabs      Json
  html      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
